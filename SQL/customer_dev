create external table Customer_dev(
reg_dttm string,
id int,
first_name string,
last_name string,
email string,
gender string,
ip_address string,
cc string,
country string,
birthdate string,
salary double,
title string,
comments string
)
PARTITIONED BY(Inputdate string, flag STRING)
stored as parquet
LOCATION '/user/hadoop/cust_dev/';
